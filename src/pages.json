{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/my/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/album/index",
      "style": {
        "navigationBarTitleText": "专辑详情"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#3D8CF1",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [{
      "pagePath": "pages/index/index",
      "iconPath": "static/image/tabbar/Homehomepagemenu.png",
      "selectedIconPath": "static/image/tabbar/Homehomepagemenu_active.png",
      "text": "首页"
    }, {
      "pagePath": "pages/my/index",
      "iconPath": "static/image/tabbar/my.png",
      "selectedIconPath": "static/image/tabbar/my_active.png",
      "text": "我的"
    }]
  }
}
